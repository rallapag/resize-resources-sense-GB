<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="layout_style_dialer_keypad">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">3.0px</item>
        <item name="android:layout_marginTop">4.0px</item>
        <item name="android:layout_marginRight">3.0px</item>
        <item name="android:layout_marginBottom">0.0px</item>
    </style>
    <style name="image_style_dialer_button">
        <item name="android:background">@drawable/htc_dial_button</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">0.0px</item>
        <item name="android:layout_marginTop">1.0px</item>
        <item name="android:layout_marginRight">3.0px</item>
        <item name="android:layout_marginBottom">1.0px</item>
    </style>
    <style name="text_style_dialer_button_number">
        <item name="android:textSize">29.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="text_style_dialer_button_phonetic_alphabet">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="text_style_dialer_button_alphabet">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ff707070</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">28.0px</item>
    </style>
    <style name="text_style_dialer_button_zero_number">
        <item name="android:textSize">29.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">2.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="image_style_dialer_button_symbol">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="image_style_dialer_button_symbol_plus">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40.0sp</item>
    </style>
    <style name="image_style_dialer_button_symbol_voicemail">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">28.0px</item>
    </style>
</resources>
